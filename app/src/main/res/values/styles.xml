<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor" tools:targetApi="lollipop">#ffffff</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowActivityTransitions" tools:targetApi="lollipop">true</item>
        <item name="android:windowSoftInputMode">adjustResize|adjustPan</item>
    </style>

    <style name="Dialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="windowNoTitle">true</item>
    </style>

    <style name="StatusBarTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimaryDark">#f1f1f1</item>
    </style>

    <style name="CheckBoxTheme" parent="ThemeOverlay.AppCompat.Dark">
        <item name="colorAccent">#000000</item>
        <item name="android:textColorSecondary">#000000</item>
    </style>

    <style name="TablayoutText" parent="Widget.Design.TabLayout">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#95989A</item>
    </style>



</resources>