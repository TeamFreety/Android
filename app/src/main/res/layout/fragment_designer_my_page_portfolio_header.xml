<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="90dp"
    android:orientation="vertical">


        <TextView
            android:id="@+id/my_page_style_career_title"
            android:text="경력사항"
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="25dp"/>

        <ImageButton
            android:id="@+id/my_page_style_career_edit_btn"
            android:layout_toRightOf="@+id/my_page_style_career_title"
            android:layout_marginLeft="5dp"
            android:layout_width="11.5dp"
            android:layout_height="11.5dp"
            android:layout_marginTop="26dp"
            android:src="@drawable/mypage_writing"/>


    <EditText
        android:id="@+id/edit_my_page_port_career"
        android:layout_below="@+id/my_page_style_career_title"
        android:layout_width="wrap_content"
        android:textSize="11sp"
        android:textColor="#95989A"
        android:layout_marginLeft="25dp"
        android:layout_height="wrap_content"
        android:lineSpacingMultiplier="1.3"
        android:text="퍼스널 펌, 컬러 전문(5년)">

    </EditText>
</RelativeLayout>
